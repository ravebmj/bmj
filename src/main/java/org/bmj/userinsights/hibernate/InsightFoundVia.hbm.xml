<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="org.bmj.userinsights.entity.InsightFoundVia" table="insight_found_via">
		<id name="id" column="insight_found_via_id">
			<generator class="native"/>
		</id>
		<property name="foundViaValue" column="found_via_other_value"/>
		<property name="addedUser" column="added_user"/>
		<property name="addedDate" column="added_date" type="date"/>
	    <many-to-one name="insightDetail" class="org.bmj.userinsights.entity.InsightDetail" fetch="select">
            <column name="insight_id" not-null="true" />
        </many-to-one>
         <many-to-one name="foundVia" class="org.bmj.userinsights.entity.FoundVia" fetch="select">
            <column name="found_via_id" not-null="true" />
        </many-to-one>
	</class>

</hibernate-mapping>