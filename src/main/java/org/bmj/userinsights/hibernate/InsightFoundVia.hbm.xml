<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="org.bmj.userinsights.entity.InsightFoundVia" table="insight_geographies">
		<id name="id" column="insight_found_via">
			<generator class="native"/>
		</id>
		<property name="insightId" column="insight_id"/>
		<property name="foundViaId" column="found_via_id"/>
		<property name="foundViaValue" column="found_via_other_value"/>
		<property name="addedUser" column="added_user"/>
		<property name="addedDate" column="added_date" type="date"/>
	
	</class>

</hibernate-mapping>